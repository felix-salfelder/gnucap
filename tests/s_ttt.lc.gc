load c_debug.so

param a=1
param q=1

build
L1 0 nosc .01m ic=0.00
.ttcap C1 nosc 0 .01m ic=1
.

options ttsteporder=3

param TTend={3*tend}
param TTend=10m
param tend=62.83u

* hack, don't have frame detection
param tend=62.8525u

nodelist

print dc v(c1) i(l1)
print tran v(c1) i(l1) v0t(0)
print tw v(c1) i(l1) ttorder(c1) ttmethod(c1)
dc uic

ttr 10u {TTend} {tend/100} {tend} new trace=g > out.out tran cont > tr.out

end


echo >> out.out
echo >> tr.out

dc uic
ttr 0 {TTend} {tend/10} {tend} * 1.5 new trace=a >> out.out tran cont  >> tr.out

end
