#!/bin/sh
dir=`dirname $0`
top="@abs_top_builddir@"
export LD_LIBRARY_PATH=$top/src/.libs:$top/modules/.libs:$top/apps/.libs \
       GNUCAP_CPPFLAGS='-I@abs_top_builddir@/include -I@abs_top_srcdir@/include \
		                  -I@abs_builddir@ -I@abs_srcdir@' \
       GNUCAP_MAKE="$top/main/gnucap.mk";
$dir/gnucap.real "$@"
