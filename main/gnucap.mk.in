#!@WHICH_MAKE@ -f

help:
	@echo "$(lastword $(MAKEFILE_LIST)) <target>"

prefix = @prefix@
GNUCAP_MAKEDIR = $(prefix)/share/gnucap/make
GNUCAP_CPPFLAGS = @GNUCAP_CPPFLAGS@
# not yet, almost
# GNUCAP_LDFLAGS = @GNUCAP_LDFLAGS@
# GNUCAP_LIBS = -lgnucap
AM_V_CXX_ = @echo CXX $@;
AM_V_CXX_0 = @echo CXX $@;
AM_V_CXX = $(AM_V_CXX_$V)
CXX = @CXX@
CXXFLAGS = @CXXFLAGS@

# non GNUCAP_-prefixed variables are meant to override stuff.
%.so: %.cc
	$(AM_V_CXX)$(CXX) $(CPPFLAGS) $(GNUCAP_CPPFLAGS) -DADD_VERSION $(CXXFLAGS) $(GNUCAP_CXXFLAGS) $< -shared -fPIC -DPIC $(LDFLAGS) $(GNUCAP_LDFLAGS) $(LIBS) $(GNUCAP_LIBS) -o $@
